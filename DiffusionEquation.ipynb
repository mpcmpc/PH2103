    "#    Temp[int(N/2)-i]= TMax\n",
    "#    Temp[int(N/2)+i]= TMax\n",
    "#############################\n",
    "\n",
    "'Here we plot to intial condition, to check that everything is as expected'\n",
    "plt.figure()\n",
    "plt.xlim(0,1000)\n",
    "plt.xlabel('X')\n",
    "plt.ylabel('Temperature')\n",
    "\n",
    "plt.plot(Temp,'r-')"
